{
  "$schema": "http://datafactories.schema.management.azure.com/schemas/2015-09-01/Microsoft.DataFactory.Table.json",
  "name": "FakeOrdersClean",
  "properties": {
    "type": "AzureDataLakeStore",
    "linkedServiceName": "DataLakeStore",
    "structure": [],
    "typeProperties": {
      "folderPath": "Cleaned",
      "fileName": "FakeOrders{year}{month}.csv",
      "partitionedBy": [
        {
          "name": "year",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "yyyy"
          }
        },
        {
          "name": "month",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "MM"
          }
        }
      ]
    },
    "availability": {
      "frequency": "Month",
      "interval": 1
    }
  }
}